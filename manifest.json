{
  "manifest_version": 3,
  "name": "remove-google-search-summary",
  "version": "1.0.0",
  "description": "Remove google search summary",
  "icons": {},
   "browser_specific_settings": {
    "gecko": {
      "id": "@remove-google-search-summary.winterstorm",
      "strict_min_version": "58.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "https://*.google.com/**"
      ]
    }
  ]
}
